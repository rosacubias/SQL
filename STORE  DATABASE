CREATE TABLE store (ID INTEGER PRIMARY KEY, color TEXT, price INTEGER, name TEXT, quantity INTEGER);

INSERT INTO store VALUES(1, "white", 2,"t-shirt", 3);

INSERT INTO store VALUES(2, "blue", 6, "jeans", 1);

INSERT INTO store VALUES(3, "red", 7, "jacket", 3);

INSERT INTO store VALUES(4, "black", 1, "shorts", 4);

INSERT INTO store VALUES(5, "black", 2, "belt",5);

SELECT * FROM store WHERE price;

SELECT SUM(price) FROM store;

SELECT * FROM store where price < 5 ORDER BY price; 

